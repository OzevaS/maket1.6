{
  "name": "maket1.6",
  "version": "1.0.0",
  "description": "webpack",
  "main": "webpack.config.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx webpack --mode development",
    "build": "cross-env NODE_ENV=production npx webpack --mode production",
    "watch": "cross-env NODE_ENV=development npx webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development npx webpack-dev-server --mode development --open",
    "predeploy": "cross-env NODE_ENV=development npm run build",
    "deploy": "gh-pages -d app"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not OperaMini All",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OzevaS/maket1.6.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "author": "ozevas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OzevaS/maket1.6/issues"
  },
  "homepage": "https://github.com/OzevaS/maket1.6#readme",
